{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch Gym Tracker API",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}/cmd/api",
            "env": {
                "AWS_REGION": "us-east-1",
                "DYNAMO_TABLE_WORKOUTS": "Workouts",
                "DYNAMO_TABLE_EXERCISES": "Exercises",
                "COGNITO_USER_POOL_ID": "",
                "COGNITO_CLIENT_ID": "",
                "PORT": "8080"
            },
            "args": [],
            "showLog": true,
            "console": "integratedTerminal"
        },
        {
            "name": "Launch with .env",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}/cmd/api",
            "envFile": "${workspaceFolder}/.env",
            "args": [],
            "showLog": true,
            "console": "integratedTerminal"
        }
    ]
}